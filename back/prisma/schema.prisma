// database
datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:password@localhost/flasksql"
}

// generator
generator client {
  provider = "prisma-client-py"
}

model Users{
    id          Int     @id @default(autoincrement())
    name        String
    role        String
    email       String
    phone       String
    keywords    String []
}

model Searchs{
    id          Int     @id @default(autoincrement())
    title       String 
    snippet     String
    link        String
    keywords    String []
}